<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="myInstantiationAwareBeanPostProcessor" class="code09.MyInstantiationAwareBeanPostProcessor"/>
    <bean id="myDestructionAwareBeanPostProcessor" class="code09.MyDestructionAwareBeanPostProcessor"/>

    <bean id="cpu" class="code09.Cup">
        <property name="name" value="杯子"/>
    </bean>

    <bean id="bowl" class="code09.Bowl">
        <property name="name" value="碗"/>
    </bean>

</beans>